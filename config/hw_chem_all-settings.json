        "comment":"This table is used to load meta data for all the papers.",
        "name":"meta_data",
        "where_clause":"mainbigcat = 'chemistry'",
        "extra_clause":"",
        "field_id":"id",
        "field_maincat":"maincat",
        "field_allcats":"catcodes",
        "field_title":"",
        "field_authors":"",
        "field_keywords":"",
        "field_agesort":"year",
        "add_missing_cats":false
    },
    "refs_table":{
        "comment":"This table is used to load the graph of references in nbody.",
        "name":"pcite",
        "field_id":"id",
        "field_refs":"refs",
        "rblob_order":false,
        "rblob_freq":false,
        "rblob_cites":true
    },
    "map_table":{
        "comment":"This table is used to store output from nbody. It can serve as input for both tiles and nbody.",
        "name":"map_data",
        "field_id":"id",
        "field_x":"x",
        "field_y":"y",
        "field_r":"r"
